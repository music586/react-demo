{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": false
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": ["standard", "plugin:react/all"],
    "plugins": ["react"],
    "rules" : {
        "indent": [2, 4],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}